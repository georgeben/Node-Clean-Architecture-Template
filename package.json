{
  "name": "clean-architecture-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build && npm run db:seed",
    "build": "babel src --delete-dir-on-start --copy-files -d dist",
    "start": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.js --env production",
    "start:dev": "nodemon \"src/index.js\"",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html --reporter=lcov mocha --require @babel/register \"src/**/**/*.spec.js\" --exit --timeout 20000 --recursive",
    "lint": "node_modules/.bin/eslint \"./src/**/*.js\"",
    "db:seed": "node \"dist/scripts/seedDb\"",
    "docs:api": "echo 'Building REST Docs...' && apidoc -i src/interfaces/http/routes/ -o docs/apidocs"
  },
  "keywords": [],
  "author": "Kurobara George",
  "dependencies": {
    "apidoc": "^0.53.1",
    "awilix": "^4.3.3",
    "awilix-express": "^4.0.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-as-promised": "^7.1.1",
    "convict": "^6.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv-extended": "^2.9.0",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "helmet": "^4.5.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.11",
    "mongoose-paginate-v2": "^1.4.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pm2": "^5.1.0",
    "request-ip": "^2.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-babel-module": "^5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0"
  },
